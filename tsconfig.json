{
  "extends":["astro/tsconfigs/strict", "astro-compress/tsconfig"],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.svelte",
    "./src/**/*.astro",
    "./src/assets/**/*",
    "./src/**/*.d.ts",
    ".astro/types.d.ts",
    "*.ts",
    "config/**/*.ts",
    "config/**/*.json",
  ],
  "exclude": [
    "./dist/**/*",
    "./node_modules/**/*",
    "*.js",
    "./.netlify/**/*",
    "./packages/**/*"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "types": [
      "gtag.js",
      "vite-plugin-pwa/info.d.ts",
      "vite-plugin-pwa/client.d.ts"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/motion/*": [
        "src/components/motion/*"
      ],
      "@/icons/*": [
        "src/components/icons/*"
      ],
      "@/ui/*": [
        "src/components/ui/*"
      ],
      "@/images/*": [
        "src/assets/images/*"
      ],
       "@/contentful/*": [
        "src/contentful/*"
      ]
    },
    "resolveJsonModule": true
  }
}
