---
import Home from '@/astro/Home/index.astro';
import { getPersonalInfo, getSeoFieldsById } from '@/contentful/queries';
import Layout from '@/layouts/Layout.astro';

const [personalInfo, seoField] = await Promise.all([
	getPersonalInfo(),
	getSeoFieldsById('4a3PYeoBpPFuf3MyhVTt8G'),
]);
---
<Layout
	{...seoField}
	siteName={seoField.title}
	image={(seoField.image as { fields: any }).fields}
	name={personalInfo.name}
>
	<Home />
</Layout>
