---
import EducationComponent from '@/components/Education.astro';
import {
	getCertifications,
	getPersonalInfo,
	getSeoFieldsById,
} from '@/contentful/queries';
import Layout from '@/layouts/Layout.astro';

const [{ name }, certifications, seoField] = await Promise.all([
	getPersonalInfo(),
	getCertifications(),
	getSeoFieldsById('4a3PYeoBpPFuf3MyhVTt8G'),
]);
---

<Layout {...seoField} image={(seoField.image as { fields: any }).fields} {name}>
	<EducationComponent {certifications} />
</Layout>
